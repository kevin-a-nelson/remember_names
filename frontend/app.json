{
  "expo": {
    "name": "Remember Names",
    "slug": "remember-names",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "sdkVersion": "37.0.0",
    "description": "An app that helps St. Olaf students remember the names of other St. Olaf students. ",
    "githubUrl": "https://github.com/kevin-a-nelson/remember_names"
  }
}
